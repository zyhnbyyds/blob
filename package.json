{
  "name": "blob",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "pm2": "pm2 start processes.json",
    "build": "nuxi build",
    "dev": "nuxi dev -p 3000 -o",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public",
    "prepare": "husky install",
    "build:docker": "docker build -f docker/Dockerfile -t blob ../"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "@bytemd/plugin-breaks": "^1.21.0",
    "@bytemd/plugin-frontmatter": "^1.21.0",
    "@bytemd/plugin-gemoji": "^1.21.0",
    "@bytemd/plugin-gfm": "^1.21.0",
    "@bytemd/plugin-highlight": "^1.21.0",
    "@bytemd/vue-next": "^1.21.0",
    "@iconify/json": "^2.2.190",
    "@nuxt/content": "^2.12.1",
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/image": "^1.4.0",
    "@nuxt/kit": "^3.10.3",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/i18n": "^8.1.1",
    "@pinia/nuxt": "^0.5.1",
    "@unocss/eslint-config": "^0.58.5",
    "@unocss/nuxt": "^0.58.5",
    "@unocss/reset": "^0.58.5",
    "@vite-pwa/nuxt": "^0.6.0",
    "@vueuse/nuxt": "^10.9.0",
    "dayjs": "^1.11.10",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "happy-dom": "^13.7.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.10.3",
    "nuxt-auth-utils": "^0.0.20",
    "nuxt-lodash": "^2.5.3",
    "nuxt-mongoose": "^1.0.5",
    "nuxt-typed-router": "^3.6.1",
    "octokit": "^3.1.2",
    "pinia": "^2.1.7",
    "sass": "^1.71.1",
    "typescript": "^5.4.2",
    "unocss-preset-scrollbar": "^0.3.1",
    "vitest": "1.3.1",
    "vue-tsc": "^2.0.6",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "vitest related --run",
      "eslint ."
    ]
  }
}
