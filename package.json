{
  "name": "blob",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "pm2": "pm2 start processes.json",
    "build": "nuxi build",
    "dev": "nuxi dev -p 3000 -o",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public",
    "prepare": "husky install",
    "build:docker": "docker build -f docker/Dockerfile -t blob ../",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.11.6",
    "@bytemd/plugin-breaks": "^1.21.0",
    "@bytemd/plugin-frontmatter": "^1.21.0",
    "@bytemd/plugin-gemoji": "^1.21.0",
    "@bytemd/plugin-gfm": "^1.21.0",
    "@bytemd/plugin-highlight": "^1.21.0",
    "@bytemd/vue-next": "^1.21.0",
    "@iconify/json": "^2.2.196",
    "@nuxt/devtools": "^1.1.5",
    "@nuxt/image": "^1.4.0",
    "@nuxt/kit": "^3.11.1",
    "@nuxtjs/color-mode": "^3.3.3",
    "@nuxtjs/i18n": "^8.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@unocss/eslint-config": "^0.58.8",
    "@unocss/nuxt": "^0.58.8",
    "@unocss/preset-tagify": "^0.58.8",
    "@unocss/reset": "^0.58.8",
    "@vite-pwa/nuxt": "^0.6.0",
    "@vueuse/nuxt": "^10.9.0",
    "dayjs": "^1.11.10",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "happy-dom": "^13.10.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.11.1",
    "nuxt-auth-utils": "^0.0.20",
    "nuxt-lodash": "^2.5.3",
    "nuxt-mongoose": "^1.0.5",
    "nuxt-typed-router": "^3.6.1",
    "octokit": "^3.1.2",
    "pinia": "^2.1.7",
    "sass": "^1.72.0",
    "shiki": "^1.2.2",
    "typescript": "^5.4.3",
    "unocss-preset-scrollbar": "^0.3.1",
    "vitepress": "1.0.0-rc.45",
    "vitest": "1.3.1",
    "vue-tsc": "^2.0.7",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "vitest related --run",
      "eslint ."
    ]
  }
}
